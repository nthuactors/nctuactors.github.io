.ytp-watermark
    display: none
.MovieContainers
    width: 100%
    height: 100vh
    position: absolute
.video-background
    background: #000;
    position: relative
    width: 100%
    height: 100%
.video-foreground,
.video-background iframe
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;

@media (min-aspect-ratio: 16/9) {
  .video-foreground { height: 300%; top: -100%; }
}
@media (max-aspect-ratio: 16/9) {
  .video-foreground { width: 300%; left: -100%; }
}
